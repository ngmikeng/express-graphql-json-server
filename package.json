{
  "name": "express-graphql-json-server",
  "version": "0.0.1",
  "description": "Express GraphQL JSON Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev:server": "nodemon --exec \"npm run lint && npm run start\"",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "json:server": "json-server --watch json/db.json --port 4000"
  },
  "author": "Minh Nguyen <ngmikeng@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^14.0.0",
    "json-server": "^0.14.0",
    "nodemon": "^1.18.4"
  },
  "devDependencies": {
    "eslint": "^5.5.0"
  }
}
